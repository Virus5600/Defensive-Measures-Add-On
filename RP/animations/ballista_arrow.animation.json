{
	"format_version": "1.8.0",
	"animations": {
		"animation.ballista_arrow.shake": {
			"loop": true,
			"bones": {
				"arrow": {
					"rotation": ["variable.shake_power - query.target_x_rotation", "-query.target_y_rotation", 0]
				}
			}
		},
		"animation.ballista_arrow.move": {
			"loop": true,
			"bones": {
				"arrow": {
					"rotation": ["-query.target_x_rotation", "-query.target_y_rotation", 0]
				}
			}
		}
	}
}